<table class="table table-hover">
  <thead>
  <tr>
    <th>Id</th>
    <th>Nome</th>
    <th>Email</th>
    <th>Senha</th>
    <th></th>
    <th></th>
  </tr>
  </thead>
  <tbody>
  <% for usuario in @usuarios %>
      <tr>
        <td><%= usuario.id %></td>
        <td><%= usuario.nome %></td>
        <td><%= usuario.email %></td>
        <td><%= usuario.password_digest %></td>
        <td>
          <span>
            <a href="<%= edit_usuario_path usuario %>">
              alterar
            </a>
            <a href="<%= usuario_path usuario %>"
               data-method="delete"
               data-confirm="Tem Certeza?"
               class="destroy">excluir
            </a>
            </span>
        </td>
      </tr>
  <% end %>
  </tbody>
</table>